{
  "name": "starpoint",
  "version": "1.0.1",
  "description": "A server reimplementation for the global version of World Flipper",
  "main": "server.ts",
  "scripts": {
    "css": "bunx tailwindcss -i ./src/input.css -o ./web/public/tailwind.css",
    "css:watch": "bun tailwindcss -i ./src/input.css -o ./web/public/tailwind.css --watch --poll",
    "dev": "bun run css && bun src/server.ts",
    "cdn": "bun src/validate_cdn.ts",
    "unzip": "bun src/unzip_cdn.ts",
    "debug": "bun run css && npm run css:watch & bun run debug:ts-only",
    "debug:ts-only": "bun --inspect src/server.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.17.11",
    "@types/readline-sync": "^1.4.8",
    "@types/unzipper": "^0.10.10",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@fastify/multipart": "^9.0.1",
    "@fastify/static": "^8.0.3",
    "cli-progress": "^3.12.0",
    "fastify": "^5.2.0",
    "msgpackr": "^1.11.2",
    "readline-sync": "^1.4.10",
    "tailwindcss": "^3.4.17",
    "unzipper": "^0.12.3"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
